@model Framework.Infrastructure.Models.Result.ReturnModel<LogR.Common.Models.Stats.DashboardSummary>


<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Logger Dashboard
            <small>1.0</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <!-- Info boxes -->
        <div class="row">
            <div class="col-md-2 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-green"><i class="ion ion-ios-cart-outline"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">App Total Logs</span>
                        <span class="info-box-number"><a href="/app-logs">@Model.Result.TotalAppLogCount</a></span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->

            <div class="col-md-2 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-red"><i class="fa fa-bug"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">App Errors</span>
                        <span class="info-box-number"><a href="/app-logs?logtype=@Framework.Infrastructure.Constants.LogLevel.Error">@Model.Result.ErrorAppLogCount</a> </span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>

            <div class="col-md-2 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-red"><i class="fa fa-bug"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">App SQL Errors</span>
                        <span class="info-box-number"><a href="/app-logs?logtype=@Framework.Infrastructure.Constants.LogLevel.SqlError">@Model.Result.ErrorSqlAppLogCount</a></span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>

            <!-- /.col -->
            <div class="col-md-2 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-aqua"><i class="fa fa-exclamation-triangle"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">App Warnings</span>
                        <span class="info-box-number"><a href="/app-logs?logtype=@Framework.Infrastructure.Constants.LogLevel.Warning">@Model.Result.WarningAppLogCount</a></span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->


            <div class="col-md-2 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-green"><i class="ion ion-ios-cart-outline"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">Perf Total Logs</span>
                        <span class="info-box-number"><a href="/performance-logs">@Model.Result.TotalPerformanceLogCount</a></span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->

            <div class="col-md-2 col-sm-6 col-xs-12">
                <div class="info-box">
                    <span class="info-box-icon bg-red"><i class="fa fa-bug"></i></span>

                    <div class="info-box-content">
                        <span class="info-box-text">Perf Error Logs</span>
                        <span class="info-box-number"><a href="/performance-logs?status=@Framework.Infrastructure.Constants.LogLevel.Error">@Model.Result.ErrorPerformanceLogCount</a></span>
                    </div>
                    <!-- /.info-box-content -->
                </div>
                <!-- /.info-box -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
            <!-- Left col -->
            <div class="col-md-12">
                <!-- /.row -->
                <!-- TABLE: LATEST ORDERS -->
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Latest App Error Logs</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="table-responsive">
                            
                                    
                        

                                <table class="table no-margin">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Severity</th>
                                        <th>MachineName</th>
                                        <th>Message</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var current in Model.Result.LastestErrorAppLogs)
                                    {
                                        <tr>
                                            <td>@current.Longdate</td>
                                            <td><span class="label label-danger label-default">@current.Severity</span></td>
                                            <td>@current.MachineName</td>
                                            <td>@current.Message</td>
                                        </tr>
                                    }
                                </tbody>

                            </table>
                        </div>
                        <!-- /.table-responsive -->
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer clearfix">
                        <a href="javascript:void(0)" class="btn btn-sm btn-info btn-flat pull-left">&gt;View</a>
                    </div>
                    <!-- /.box-footer -->
                </div>
                <!-- /.box -->
            </div>

            <div class="col-md-12">
                <!-- /.row -->
                <!-- TABLE: LATEST ORDERS -->
                <div class="box box-info">
                    <div class="box-header with-border">
                        <h3 class="box-title">Latest Performance Error Logs</h3>

                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool btn-default" data-widget="collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-box-tool btn-default" data-widget="remove"><i class="fa fa-times"></i></button>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="table-responsive">                                    
                                <table class="table no-margin">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>MachineName</th>
                                        <th>Status</th>
                                        <th>Message</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var current in Model.Result.LastestErrorPerformanceLogs)
                                    {
                                    <tr>
                                            <td>@current.Longdate</td>
                                            <td>@current.MachineName</td>
                                            <td>@current.Status</td>
                                            <td>@current.AdditionalMessage</td>
                                    </tr>
                                    }
                                    </tbody>
                            </table>
                        </div>
                        <!-- /.table-responsive -->
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer clearfix">
                        <a href="javascript:void(0)" class="btn btn-sm btn-info btn-flat pull-left">&gt;View</a>
                    </div>
                    <!-- /.box-footer -->
                </div>
                <!-- /.box -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>

    <!-- /.content -->
</div>
<!-- /.content-wrapper -->